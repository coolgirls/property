<!--pages/register/register.wxml-->
<view class="register">
  <view class="register-bg">
    <image src='../../images/lgRg.png'></image>
  </view>
  <view class="register-item">
    <view class="list-item">
      <label>账号</label>
      <input type='text' placeholder='请输入账号'></input>
    </view>
    <view class="list-item">
      <label>密码</label>
      <input type='text' placeholder='请输入密码' password></input>
    </view>
    <view class="list-item">
      <label>姓名</label>
      <input type='text' placeholder='请输入姓名'></input>
    </view>
    <view class="list-item">
      <label>楼栋</label>
      <input type='text' placeholder='请输入楼栋'></input>
    </view>
    <view class="list-item">
      <label>单元</label>
      <input type='text' placeholder='请输入单元'></input>
    </view>
    <view class="list-item">
      <label>门牌号</label>
      <input type='text' placeholder='请输入门牌号'></input>
    </view>
  </view>
  <view class='register-btn'>注册</view>
</view>

